{
  "name": "ai-agent-cloud",
  "version": "1.0.0",
  "description": "Cloud-based AI Coding Agent",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "dev": "npm run build && electron .",
    "build": "tsc",
    "start": "electron .",
    "dist": "npm run build && electron-builder"
  },
  "build": {
    "appId": "com.ai.agent.cloud",
    "productName": "AI Coding Agent",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "public/**/*",
      "package.json"
    ],
    "win": {
      "target": "portable"
    }
  },
  "dependencies": {
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "e2b": "^0.16.2",
    "eventemitter3": "^5.0.1",
    "express": "^4.18.2",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "simple-git": "^3.22.0",
    "uuid": "^9.0.1",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/electron": "^1.4.38",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/uuid": "^9.0.7",
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
